{
    "compilerOptions": {
        "target": "es5",
        "module": "es6",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "jsx": "react",
        "jsxFactory": "createCell",
        "lib": ["DOM", "DOM.Iterable"]
    },
    "include": ["./source/**/*"],
    "typedocOptions": {
        "name": "BootCell",
        "mode": "file",
        "readme": "./ReadMe.md",
        "out": "docs/"
    }
}
